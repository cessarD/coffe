    "graphql-tools": "^7.0.4",
    "helmet": "^4.4.1",
    "react": "^16.0.0",
    "react-awesome-slider": "^4.1.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-whirligig": "^6.6.15"
  },
  "devDependencies": {
    "prettier": "2.2.1"
  },
  "repository": {
    "type": "git",
